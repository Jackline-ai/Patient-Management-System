package com.patientmis.dto;

public class ServiceRegistrationEvent {
    private Long serviceId;
    private String serviceName;
    private double quantity;
    private double price;
    private double discount;
    private double total;
    private String eventType;

    public ServiceRegistrationEvent() {
    }

    public ServiceRegistrationEvent(Long serviseId, String serviceName, double quantity, double price, double discount,
	    double total, String eventType) {
	this.serviceId = serviceId;
	this.serviceName = serviceName;
	this.quantity = quantity;
	this.price = price;
	this.discount = discount;
	this.total = total;
	this.eventType= eventType;

    }

    public Long getServiceId() {
	return serviceId;
    }

    public String getServiceName() {
	return serviceName;
    }

    public double getQuantity() {
	return quantity;
    }

    public double getPrice() {
	return price;
    }

    public double getDiscount() {
	return discount;
    }

    public double getTotal() {
	return total;
    }

    public void setServiceId(Long serviceId) {
	this.serviceId = serviceId;
    }

    public void setServiceName(String serviceName) {
	this.serviceName = serviceName;
    }

    public void setQuantity(double quantity) {
	this.quantity = quantity;
    }

    public void setPrice(double price) {
	this.price = price;
    }

    public void setDiscount(double discount) {
	this.discount = discount;
    }

    public void setTotal(double total) {
	this.total = total;
    }

    public String getEventType() {
        return eventType;
    }

    public void setEventType(String eventType) {
        eventType = eventType;
    }

}
